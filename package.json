{
  "name": "vue3-ts-vite-ssr-starter",
  "version": "2.0.2",
  "description": "Use vue3 vite typescript eslint SSR vuex vue-router element-plus scss",
  "author": "vok123",
  "scripts": {
    "dev": "node server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.js --outDir dist/server",
    "serve": "cross-env NODE_ENV=production node server",
    "preview": "npm run build && npm run serve"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "element-plus": "1.2.0-beta.4",
    "vue": "^3.2.23",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "vuex-router-sync": "^6.0.0-rc.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/node": "^14.14.39",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^1.10.1",
    "@vitejs/plugin-vue-jsx": "^1.3.0",
    "@vue/compiler-sfc": "^3.2.23",
    "@vue/server-renderer": "^3.2.23",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-plugin-vue": "^7.20.0",
    "express": "^4.17.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "^1.32.8",
    "typescript": "^4.4.4",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^2.6.14",
    "vite-plugin-eslint": "^1.3.0",
    "vue-eslint-parser": "^8.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vok123/vue3-ts-vite-ssr-starter.git"
  },
  "license": "MIT"
}
